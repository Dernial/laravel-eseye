{
    "name": "dernial/laravel-eseye",
    "description": "A simple Laravel wrapper for SeAT's Eseye package",
    "keywords": ["laravel", "laravel-5", "esi", "eseye", "eve", "eve-online"],
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Cooke",
            "email": "michael.frank.cooke@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": "^7",
        "eveseat/eseye": "^2.2.1"
    },
    "autoload": {
        "psr-4": {
            "MichaelCooke\\LaravelEseye\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MichaelCooke\\LaravelEseye\\LaravelEseyeServiceProvider"
            ],
            "aliases": {
                "Eseye": "MichaelCooke\\LaravelEseye\\Facades\\Eseye"
            }
        }
    }
}
